# Copy to .env and fill in values
#
# Windows PowerShell:
#   Copy-Item env.example .env
#
# macOS/Linux:
#   cp env.example .env

# Required (core data source)
ATTOM_API_KEY=

# Optional: Oxylabs Web Scraper API (fallback/enrichment)
OXYLABS_ENABLED=false
OXYLABS_USERNAME=
OXYLABS_PASSWORD=

# Optional: Estated API (fallback)
ESTATED_ENABLED=false
ESTATED_API_KEY=

# Optional: PropertyRadar API (investor data enrichment)
PROPERTYRADAR_ENABLED=false
PROPERTYRADAR_API_KEY=

# Optional: Google Maps (Street View images)
GOOGLE_MAPS_API_KEY=

# Optional: Email sending
EMAIL_ENABLED=false
EMAIL_SMTP_SERVER=smtp.gmail.com
EMAIL_SMTP_PORT=587
EMAIL_SMTP_USERNAME=
EMAIL_SMTP_PASSWORD=
EMAIL_FROM_ADDRESS=
EMAIL_FROM_NAME=MLS Comp Bot

# Optional: Flask secret key
SECRET_KEY=


